<script type="text/javascript">
	$(document).ready(function(){
		$.ajax({
			type : 'POST',
			url : '/chapter06_Maven/user/getList.do', 
			data : "id"+id,		/* 서버로 값을 보낼 때 */
			dataType : 'json',	/* list타입으로 db에서 받아와 json으로 변경 */
			/* 서버로부터 결과는 받을 때 */
			success : function(data){
				alert(JSON.stringify(data));
			}
		});
	});
