<script type="text/javascript">
$(window).scroll(function() {  
    var scroll = $(window).scrollTop();
    if (scroll >= 100) {
        $(".romi_category").addClass("romi_fixed");
    } else {
        $(".romi_category").removeClass("romi_fixed");
    }
});

